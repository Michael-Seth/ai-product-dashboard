{
  "name": "@ai-product-dashboard/source",
  "version": "0.0.1",
  "type": "commonjs",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "nx build",
    "build:all": "npm run build:react-web-component && npm run build:angular && npm run build:react",
    "build:angular": "nx build angular-dashboard",
    "build:react": "nx build react-recommender",
    "build:react-web-component": "nx build-web-component react-recommender",
    "build:prod": "nx run-many --target=build --configuration=production --parallel=3",
    "build:analyze": "npm run build:prod && npm run analyze:bundles",
    "analyze:bundles": "node scripts/analyze-build.js",
    "deploy:prod": "node scripts/deploy-production.js",
    "test": "nx test",
    "test:integration": "nx test integration-tests",
    "test:integration:watch": "nx test:watch integration-tests",
    "test:integration:coverage": "nx test:coverage integration-tests",
    "test:integration:runner": "node integration-tests/run-integration-tests.js",
    "test:all": "nx test && npm run test:integration",
    "dev-server": "node dev-server.js",
    "dev": "concurrently \"npm run dev-server\" \"nx serve angular-dashboard\"",
    "remove-comments": "node scripts/remove-comments.js",
    "remove-comments:dry": "node scripts/remove-comments.js --dry-run",
    "remove-comments:all": "node scripts/remove-comments.js --no-preserve",
    "remove-comments:verbose": "node scripts/remove-comments.js --verbose",
    "remove-comments:advanced": "node scripts/remove-comments-advanced.js",
    "remove-comments:advanced:dry": "node scripts/remove-comments-advanced.js --dry-run",
    "remove-comments:ts": "node scripts/remove-comments-advanced.js --pattern '**/*.ts'",
    "remove-comments:js": "node scripts/remove-comments-advanced.js --pattern '**/*.js'",
    "remove-console": "node scripts/remove-console-logs.js",
    "remove-console:dry": "node scripts/remove-console-logs.js --dry-run",
    "remove-console:all": "node scripts/remove-console-logs.js --all",
    "remove-console:verbose": "node scripts/remove-console-logs.js --verbose",
    "remove-console:advanced": "node scripts/remove-console-logs-advanced.js",
    "remove-console:advanced:dry": "node scripts/remove-console-logs-advanced.js --dry-run",
    "remove-console:prod": "node scripts/remove-console-logs-advanced.js --env production",
    "remove-console:dev": "node scripts/remove-console-logs-advanced.js --env development",
    "remove-console-symbols": "node scripts/remove-console-symbols.js",
    "remove-console-symbols:dry": "node scripts/remove-console-symbols.js --dry-run",
    "remove-console-symbols:emojis": "node scripts/remove-console-symbols.js --emojis-only",
    "remove-console-symbols:symbols": "node scripts/remove-console-symbols.js --symbols-only",
    "patch-console-symbols": "node scripts/console-symbol-patch.js --apply",
    "patch-console-symbols:revert": "node scripts/console-symbol-patch.js --revert",
    "organize-docs": "node scripts/organize-docs.js",
    "organize-docs:dry": "node scripts/organize-docs.js --dry-run",
    "organize-docs:revert": "node scripts/organize-docs.js --revert",
    "test-ai": "node scripts/test-ai-providers.js",
    "test-ai:verbose": "node scripts/test-ai-providers.js --verbose",
    "test-ai:openai": "node scripts/test-ai-providers.js --provider openai",
    "test-ai:grok": "node scripts/test-ai-providers.js --provider grok",
    "test-ai:claude": "node scripts/test-ai-providers.js --provider claude"
  },
  "nx": {
    "includedScripts": []
  },
  "dependencies": {
    "openai": "^6.5.0",
    "tslib": "^2.3.0",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "@nx/eslint-plugin": "21.6.5",
    "@nx/jest": "21.6.5",
    "glob": "10.4.5"
  },
