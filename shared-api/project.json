{
  "name": "shared-api",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "shared-api/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/shared-api",
        "main": "shared-api/src/index.ts",
        "tsConfig": "shared-api/tsconfig.lib.json",
        "assets": ["shared-api/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "shared-api/jest.config.ts"
      }
    }
  }
}
