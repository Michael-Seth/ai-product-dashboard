{
  "version": 2,
  "builds": [
    {
      "src": "dist/angular-dashboard/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/dist/angular-dashboard/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/angular-dashboard/browser/index.html"
    }
  ]
}
